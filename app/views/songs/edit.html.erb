

<h2> Edit Song Form </h2>
<%= form_for(@song) do |f| %>
    <label>Song Name: </label><br>
    <%= f.text_field :name %> <br><br>

    <label>Artist: </label><br>
    <%= f.text_field :artist_id %> <br><br>

    <label>Genre: </label><br>
    <%= f.text_field :genre_id %> <br><br>

    <%= f.submit %>

<% end %>