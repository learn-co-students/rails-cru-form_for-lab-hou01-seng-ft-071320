<h1> Song Form </h1>

<%= form_for @song do |f| %>
    <%= f.label :name, "My song name" %>
    <%= f.text_field :name %> <br><br>
    <%= f.label :artist_id, "My Artist" %>
    <%= f.text_field :artist_id %> <br><br>
    <%= f.label :genre_id, "My Genre" %>
    <%= f.text_field :genre_id %> <br><br>
    <%= f.submit "Create Song" %>
<% end %>